{
    "title": "iOS知识总结",
    "description": "iOS知识总结",
    "author": "liyb",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "5.1.4",
    "root": ".",
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "expandable-chapters-small",
        "back-to-top-button",
        "-sharing",
        "github",
        "code",
        "lightbox",
        "hide-element",
        "chapter-fold",
        "auto-scroll-table"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/liyb93"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        }
    },
    "lightbox": {
        "includeJQuery": false,
        "sameUuid": true,
        "options": {
            "resizeDuration": 500,
            "wrapAround": false
        }
    }
}
